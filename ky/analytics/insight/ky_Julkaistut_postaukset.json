{
    "$id": "ky_Julkaistut_postaukset",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Julkaistut postaukset",
    "description": "Tarkastele Kontaktin kautta Google-yritysprofiiliin ja Facebook-sivulle julkaistujen postausten määrää toimipisteittäin.",
    "query": {
        "metrics": [
            "yext/SOCIAL_ACTIVITIES"
        ],
        "fields": [
            "entityId",
            "name"
        ],
        "dimensions": [
            "yext/LOCATIONS"
        ],
        "filters": [
            {
                "filter": "yext/date",
                "in": "MONTHS_13"
            }
        ]
    },
    "display": {
        "table": {
            "showSummary": true
        }
    }
}